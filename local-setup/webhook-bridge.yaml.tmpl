---
apiVersion: v1
kind: Service
metadata:
  name: stackit-webhook
  namespace: default
spec:
  ports:
    - port: 443
      targetPort: 8443 # The port our local Go process will listen on
---
apiVersion: v1
kind: Endpoints
metadata:
  name: stackit-webhook
  namespace: default
subsets:
  - addresses:
      - ip: HOST_IP_PLACEHOLDER
    ports:
      - port: 8443